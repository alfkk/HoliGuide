<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>HoliGuide</servlet-name>
		<servlet-class>com.HoliGuide.HoliGuideServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HoliGuide</servlet-name>
		<url-pattern>/holiguide</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>app/index.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>ViewGuideList</servlet-name>
		<servlet-class>com.HoliGuide.servlet.ViewGuideServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewGuideList</servlet-name>
		<url-pattern>/viewguide</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ViewItineraryList</servlet-name>
		<servlet-class>com.HoliGuide.servlet.ViewItineraryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewItineraryList</servlet-name>
		<url-pattern>/viewitinerarylist</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ViewTourGuideItinerary</servlet-name>
		<servlet-class>com.HoliGuide.servlet.ViewTourGuideItinerary</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewTourGuideItinerary</servlet-name>
		<url-pattern>/viewtourguideitinerarylist</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GuideMyItinerary</servlet-name>
		<servlet-class>com.HoliGuide.servlet.GuideMyItinerary</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GuideMyItinerary</servlet-name>
		<url-pattern>/guidemyitinerary</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CreateItinerary</servlet-name>
		<servlet-class>com.HoliGuide.servlet.CreateItineraryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateItinerary</servlet-name>
		<url-pattern>/createitinerary</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ReviewItinerary</servlet-name>
		<servlet-class>com.HoliGuide.servlet.ReviewItinerary</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReviewItinerary</servlet-name>
		<url-pattern>/reviewitinerary</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>TourGuideFeedback</servlet-name>
		<servlet-class>com.HoliGuide.servlet.GiveFeedbackServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TourGuideFeedback</servlet-name>
		<url-pattern>/tourguidefeedback</url-pattern>
	</servlet-mapping>

</web-app>